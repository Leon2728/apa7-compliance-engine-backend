# LLM Configuration
# Set to true to enable LLM features (coach endpoint with AI support)
# When false (or not set), the backend works exactly as before without LLM
LLM_ENABLED=false

# OpenAI Configuration (required if LLM_ENABLED=true)
# Your OpenAI API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=

# OpenAI Model to use (default: gpt-4, options: gpt-4, gpt-3.5-turbo, etc.)
OPENAI_MODEL=gpt-4

# OpenAI API call timeout in seconds (default: 30)
OPENAI_TIMEOUT=30

# Application Settings
DEBUG=false
